#include <stdio.h>

int main() {
    int number, digit;
    long long product = 1; // Use long long for the product to prevent overflow
    
    printf("Enter a number: ");
    scanf("%d", &number);

    if (number == 0) {
        printf("The number 0 has no odd digits.\n");
        return 0;
    }

    // Handle negative numbers by converting them to positive
    if (number < 0) {
        number = -number;
    }

    // Loop through each digit of the number
    while (number > 0) {
        digit = number % 10; // Get the last digit

        // Check if the digit is odd
        if (digit % 2 != 0) {
            product *= digit; // Multiply the odd digit with the product
        }
        
        number /= 10; // Remove the last digit
    }
    
    // If no odd digits were found, the product will still be 1.
    // We check this condition to provide a more informative message.
    if (product == 1) {
        printf("The number contains no odd digits.\n");
    } else {
        printf("The product of the odd digits is: %lld\n", product);
    }

    return 0;
}