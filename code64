#include <stdio.h>

int main() {
    long long num;
    int frequency[10] = {0}; // Initializes all 10 elements to 0
    int digit;

    // 1. Input the integer
    printf("Enter an integer number: ");
    scanf("%lld", &num);

    // Make the number positive if it's negative
    if (num < 0) {
        num = -num;
    }
    
    // Handle the special case where the input is 0
    if (num == 0) {
        printf("The most frequent digit is 0\n");
        return 0;
    }

    // 2. Extract digits and update their frequency
    long long temp_num = num;
    while (temp_num > 0) {
        digit = temp_num % 10; // Get the last digit
        frequency[digit]++;     // Increment its count
        temp_num /= 10;         // Remove the last digit
    }

    // 3. Find the digit with the highest frequency
    int most_frequent_digit = 0;
    int max_count = 0;

    for (int i = 0; i < 10; i++) {
        if (frequency[i] > max_count) {
            max_count = frequency[i];
            most_frequent_digit = i;
        }
    }

    // 4. Print the result
    printf("The most frequent digit is %d\n", most_frequent_digit);

    return 0;
}