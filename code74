#include <stdio.h>

int main() {
    int rows, cols;
    int i, j;

    // Get the dimensions of the original matrix
    printf("Enter the number of rows: ");
    scanf("%d", &rows);
    printf("Enter the number of columns: ");
    scanf("%d", &cols);

    // Declare the original matrix and the transpose matrix
    // Note that the dimensions for the transpose are swapped
    int matrix[rows][cols];
    int transpose[cols][rows];

    // Read the elements of the original matrix
    printf("\nEnter the elements of the matrix:\n");
    for (i = 0; i < rows; i++) {
        for (j = 0; j < cols; j++) {
            printf("Enter element at position [%d][%d]: ", i, j);
            scanf("%d", &matrix[i][j]);
        }
    }

    // --- Core Logic: Create the transpose ---
    for (i = 0; i < rows; i++) {
        for (j = 0; j < cols; j++) {
            transpose[j][i] = matrix[i][j];
        }
    }

    // --- Display the Results ---
    printf("\n--- Original Matrix (%d x %d) ---\n", rows, cols);
    for (i = 0; i < rows; i++) {
        for (j = 0; j < cols; j++) {
            printf("%d\t", matrix[i][j]);
        }
        printf("\n");
    }

    printf("\n--- Transpose Matrix (%d x %d) ---\n", cols, rows);
    for (i = 0; i < cols; i++) {
        for (j = 0; j < rows; j++) {
            printf("%d\t", transpose[i][j]);
        }
        printf("\n");
    }

    return 0;
}