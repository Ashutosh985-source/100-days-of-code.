#include <stdio.h>

int main() {
    int i, j;

    for (i = 5; i >= 1; i--) {         // Outer loop: rows from 5 to 1
        for (j = 1; j <= i; j++) {     // Inner loop: print '*' i times
            printf("*");
        }
        printf("\n");                  // Newline after each row
    }

    return 0;
}
