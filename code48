#include <stdio.h>

int main() {
    int i, j;

    for (i = 1; i <= 5; i++) {         // Outer loop for rows
        for (j = 1; j <= (i < 5 ? i : 4); j++) { // Inner loop for numbers
            printf("%d", j);
        }
        printf("\n");                  // Newline after each row
    }

    return 0;
}
