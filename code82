#include <stdio.h>

int main() {
    char str[100];

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);  // Read string including spaces

    // Print each character on a new line
    for (int i = 0; str[i] != '\0'; i++) {
        // Skip newline character from fgets
        if (str[i] == '\n') continue;
        printf("%c\n", str[i]);
    }

    return 0;
}
