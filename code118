#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int nums[n];
    
    for (int i = 0; i < n; i++)
        scanf("%d", &nums[i]);

    int total = n * (n + 1) / 2;  // Sum of numbers from 0 to n
    int sum = 0;

    for (int i = 0; i < n; i++)
        sum += nums[i];

    printf("%d", total - sum);
    return 0;
}
